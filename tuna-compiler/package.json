{
  "name": "tuna-compiler",
  "version": "0.1.1",
  "scripts": {
    "compile": "tspeg parser.peg src/main/parser.ts && tsc -b",
    "pretest": "npm run compile",
    "prepublish": "npm run compile",
    "test": "jest --maxWorkers=50%"
  },
  "author": "Jeremy",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.8",
    "@types/mongodb": "^3.5.33",
    "jest": "^26.2.2",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.4",
    "tspeg": "^2.1.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "conder_core": "https://github.com/Conder-Systems/conder/releases/download/v0.2.2/conder-api.tar.gz"
  },
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "**/test/**/*.[jt]s",
      "**/?(*.)+(spec|test).[tj]s"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts"
}
